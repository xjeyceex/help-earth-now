generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./.app/prisma/dev.db"  
}

model users{
  id String @id @default(cuid())
  firstName String?
  lastName String?
  email String @unique
  createDate DateTime @default(now())
  updateDate DateTime @updatedAt
  isAdmin Boolean @default(false)
  password String
  isActive Boolean @default(true)
}

model Candidate {
  id          String   @id @default(cuid()) 
  firstName   String  
  lastName    String  
  description String   
  party       String    
  link        String   
  rating      String   
  state       String   
}

